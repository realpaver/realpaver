# In: Burden, R.L., Faires, J.D.
#     Numerical Analysis
#     Numerical Solutions of Nonlinear Systems of Equations, (pp. 597-640).

Variables
   x1 in [-1.0e3, 1.0e3],
   x2 in [-1.0e3, 1.0e3],
   x3 in [-1.0e3, 1.0e3];

Constraints
   3*x1 - cos(x2*x3) - 0.5 == 0,
   x1^2 - 81*(x2+0.1)^2 + sin(x3) + 1.06 == 0,
   exp(-x1*x2) + 20*x3 + (10*PI-3)/3 == 0;
