Variables
   x in [-1000, 1000],
   y in [-1000, 1000],
   z in [-1000, 1000],
   a in [-1000, 1000],
   b in [-1000, 1000],
   l in [-1000, 1000],
   p in [-1000, 1000];

Constraints
   a^2 + y^2 == p^2,
   b^2 + z^2 == l^2,
   a^2 - l^2 + 60*l == 800,
   b^2 - p^2 + 40*p == 300,
   y*(y + 20) + x^2 == 300,
   z*(z + 20) + x^2 == 800,
   x == a + b,
   30*b == l*x,
   20*a == p*x,
   p*(10+y) == 20*y,
   l*(10+z) == 30*z;
