# 1 solution

Variables
   x in [-1.0e5, 1.0e5],
   y in [-1.0e5, 1.0e5],
   l in [-1.0e5, 1.0e5],
   t in [-1.0e5, 1.0e5],
   L in [-1.0e5, 1.0e5],
   b in [-1.0e5, 1.0e5],
   r in [-1.0e5, 1.0e5],
   a in [-1.0e5, 1.0e5],
   w in [0, 1.0e5];

Constraints
   x - l*t^3 - L*w == 0,
   y - L*t - l*w^3 == 0,
   L - 1           == 0,
   l - 2           == 0,
   x - a           == 0,
   2*a - 3         == 0,
   y - b           == 0,
   b - r*t         == 0,
   w^2 - 1 + t^2   == 0;
